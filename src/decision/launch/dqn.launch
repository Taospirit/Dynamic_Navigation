<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="robot" value="create" />
    <rosparam command="load" file="$(find decision)/config/turtlebot_params.yaml" />

    <include file="$(find turtlebot_gazebo)/launch/show_$(arg robot).launch">
    
    <node pkg="decision" name="decision" type="train.py" output="screen"/>

</launch>